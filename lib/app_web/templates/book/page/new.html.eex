<%= render(AppWeb.SharedView, "nav.html", conn: @conn) %>
<div class="main">
<%= render(AppWeb.SharedView, "back-navigation.html", conn: @conn, title: "New Books") %>

<div class="sub-container">
  <%= form_for @book, Routes.book_page_path(@conn, :create), fn f -> %>

  <label>Book Entries</label>
  <%= textarea f, :books, placeholder: "Format:\n#isbn,title,author,category,copies" %>
  <%= submit "Save" %>
  <% end %>
</div>
</div>
